{
  "name": "swallowtail",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:web": "bun run --filter @swallowtail/web dev  --webpack",
    "dev:api": "bun run --filter @swallowtail/api dev",
    "dev": "concurrently \"bun run dev:web\" \"bun run dev:api\""
  }
}
